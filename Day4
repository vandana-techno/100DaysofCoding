Problem:-

Solution:-

class Solution {
    public ListNode insertionSortList(ListNode head) {
        if(head == null || head.next == null) return head;
        ListNode dummy = new ListNode(0);
        ListNode cur = head;
        ListNode temp = null;
        while(cur != null){
            temp = dummy;
            while(temp.next != null && temp.next.val < cur.val){
                temp = temp.next;
            }
            ListNode next = cur.next;
            cur.next = temp.next;
            temp.next = cur;
            cur = next;
        }
        return dummy.next;
    }
}
